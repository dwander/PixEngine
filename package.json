{
  "name": "tauri-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "predev": "node scripts/generate-config.js",
    "dev": "vite",
    "prebuild": "node scripts/bump-version.js",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "pretauri": "node scripts/generate-config.js",
    "tauri:build": "node scripts/bump-version.js && tauri build",
    "config": "node scripts/generate-config.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css}\""
  },
  "dependencies": {
    "@tanstack/react-virtual": "^3.13.12",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "^2.4.2",
    "@tauri-apps/plugin-fs": "^2.4.4",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-store": "^2.4.1",
    "dockview-react": "^4.9.0",
    "konva": "^10.0.8",
    "lucide-react": "^0.548.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-konva": "^19.2.0",
    "react-virtuoso": "^4.14.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.8.3",
    "vite": "^7.1.12"
  }
}
